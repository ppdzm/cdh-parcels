{
  "name": "ELASTICSEARCH",
  "label": "ElasticSearch",
  "description": "Elastic Search service",
  "version": "7.16.0",
  "runAs": {
    "user": "root",
    "group": "root"
  },
  "inExpressWizard": true,
  "maxInstances": 9,
  "icon": "images/elasticsearch.png",
  "compatibility": {
    "generation": 1,
    "cdhVersion": {
      "min": 5,
      "max": 6
    }
  },
  "parcel": {
    "requiredTags": [
      "elasticsearch"
    ]
  },
  "roleWithExternalLinks": [
    "ELASTICSEARCH_MASTER"
  ],
  "roles": [
    {
      "name": "ELASTICSEARCH_MASTER",
      "label": "Elastic Search Master",
      "pluralLabel": "Elastic Search Master Node",
      "jvmBased": true,
      "startRunner": {
        "program": "scripts/control.sh",
        "args": [
          "start"
        ],
        "environmentVariables": {
          "CURRENT_RULE": "elasticsearch-master",
          "ELASTICSEARCH_LOG_DIR": "${path_logs}",
          "ELASTICSEARCH_DATA_DIR": "${path_data}"
        }
      },
      "stopRunner": {
        "timeout": "90000",
        "runner": {
          "program": "scripts/control.sh",
          "args": [
            "stop"
          ],
          "environmentVariables": {
            "CURRENT_RULE": "elasticsearch-master"
          }
        }
      },
      "externalLink": {
        "name": "ELASTICSEARCH_MASTER",
        "label": "ElasticSearch Header",
        "url": "http://${host}:${http_port}/_plugin/head"
      },
      "topology": {
        "minInstances": "1",
        "requiresOddNumberOfInstances": "false",
        "placementRules": []
      },
      "logging": {
        "dir": "/var/log/elasticsearch/elasticsearch_master",
        "filename": "elasticsearch_master.log",
        "modifiable": true,
        "configName": "log.dir",
        "loggingType": "log4j"
      },
      "configWriter": {
        "generators": [
          {
            "filename": "elasticsearch.properties",
            "configFormat": "properties",
            "includedParams": [
              "cluster_name",
              "path_data",
              "path_logs",
              "max_local_storage_nodes",
              "initial_master_nodes",
              "discovery.seed_hosts",
              "http_port",
              "http_cors_enabled",
              "http_cors_allow_origin",
              "network_hosts"
            ],
            "additionalConfigs": [
              {
                "key": "node.name",
                "value": "${host}-master"
              },
              {
                "key": "node.roles",
                "value": "[ master ]"
              }
            ]
          },
          {
            "filename": "jvm.properties",
            "refreshable": false,
            "configFormat": "properties",
            "includedParams": [
              "Xms",
              "Xmx",
              "UseConcMarkSweepGC",
              "CMSInitiatingOccupancyFraction",
              "UseCMSInitiatingOccupancyOnly",
              "UseG1GC",
              "G1ReservePercent",
              "InitiatingHeapOccupancyPercent",
              "HeapDumpOnOutOfMemoryError",
              "FatalErrorLogs",
              "PrintGCDetails",
              "PrintGCDateStamps",
              "PrintTenuringDistribution",
              "PrintGCApplicationStoppedTime",
              "Xloggc",
              "UseGCLogFileRotation",
              "NumberOfGCLogFiles",
              "GCLogFileSize"
            ]
          }
        ]
      },
      "parameters": [
        {
          "name": "network_hosts",
          "label": "network_hosts",
          "description": "Set a custom host for HTTP",
          "configName": "network.host",
          "required": "true",
          "type": "string",
          "default": "0.0.0.0"
        },
        {
          "name": "http_port",
          "label": "HTTP port",
          "description": "Set a custom port for HTTP",
          "configName": "http.port",
          "required": "true",
          "type": "long",
          "default": "9200"
        },
        {
          "name": "path_data",
          "label": "Path to data directory",
          "description": "Path to directory where to store the data (separate multiple locations by comma)",
          "configName": "path.data",
          "required": "true",
          "type": "string",
          "pathType": "localDataDir",
          "default": "/var/lib/elasticsearch/elasticsearch-master"
        },
        {
          "name": "path_logs",
          "label": "Path to logs directory",
          "description": "Path to log files",
          "configName": "path.logs",
          "required": "true",
          "type": "path",
          "pathType": "localDataDir",
          "default": "/var/log/elasticsearch/elasticsearch-master"
        },
        {
          "name": "Xms",
          "label": "JVM Xms",
          "description": "Xms represents the initial size of total heap space",
          "configName": "Xms",
          "required": "true",
          "type": "string",
          "default": "-Xms2g"
        },
        {
          "name": "Xmx",
          "label": "JVM Xmx",
          "description": "Xms represents the maximum size of total heap space",
          "configName": "Xmx",
          "required": "true",
          "type": "string",
          "default": "-Xmx2g"
        },
        {
          "name": "UseConcMarkSweepGC",
          "label": "JVM UseConcMarkSweepGC",
          "description": "Enable ConcurrentMarkSweep (CMS)",
          "configName": "UseConcMarkSweepGC",
          "required": "false",
          "type": "string",
          "default": "8-13:-XX:+UseConcMarkSweepGC"
        },
        {
          "name": "CMSInitiatingOccupancyFraction",
          "label": "JVM CMSInitiatingOccupancyFraction",
          "description": "CMS Initiating Occupancy Fraction",
          "configName": "CMSInitiatingOccupancyFraction",
          "required": "false",
          "type": "string",
