          "required": "false",
          "type": "string",
          "default": "8:-XX:+PrintGCApplicationStoppedTime"
        },
        {
          "name": "Xloggc",
          "label": "JVM Xloggc",
          "description": "Xloggc",
          "configName": "Xloggc",
          "required": "false",
          "type": "string",
          "default": "8:-Xloggc:logs/gc.log"
        },
        {
          "name": "UseGCLogFileRotation",
          "label": "JVM UseGCLogFileRotation",
          "description": "UseGCLogFileRotation",
          "configName": "UseGCLogFileRotation",
          "required": "false",
          "type": "string",
          "default": "8:-XX:+UseGCLogFileRotation"
